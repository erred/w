{
  "hosting": {
    "site": "com-seankhliao",
    "public": "public/",
    "cleanUrls": true,
    "trailingSlash": true,
    "headers": [
      {
        "source": "*",
        "headers": [
          { "key": "Strict-Transport-Security", "value": "max-age=63072000; preload" },
          { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
          {
            "key": "Report-To",
            "value": "{\"group\": \"csp-endpoint\", \"max_age\": 10886400, \"endpoints\": [{\"url\":\"https://statslogger.seankhliao.com/json\"}]}"
          },
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; upgrade-insecure-requests; connect-src https://statslogger.seankhliao.com https://www.google-analytics.com; font-src https://seankhliao.com; img-src *; object-src 'none'; script-src-elem 'nonce-deadbeef2' 'nonce-deadbeef3' 'nonce-deadbeef4' https://seankhliao.com https://www.google-analytics.com https://ssl.google-analytics.com https://www.googletagmanager.com; sandbox allow-scripts; style-src-elem 'nonce-deadbeef1' https://seankhliao.com; report-to csp-endpoint; report-uri https://statslogger.seankhliao.com/json"
          },
          {
            "key": "Feature-Policy",
            "value": "accelerometer 'none'; autoplay 'none'; camera 'none'; document-domain 'none'; encrypted-media 'none'; fullscreen 'none'; geolocation 'none'; gyroscope 'none'; magnetometer 'none'; microphone 'none'; midi 'none'; payment 'none'; picture-in-picture 'none'; sync-xhr 'none'; usb 'none'; xr-spatial-tracking 'none'"
          }
        ]
      },
      {
        "source": "/blog/",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      },
      {
        "source": "/blog/**",
        "headers": [{ "key": "Cache-Control", "value": "max-age=86400" }]
      },
      {
        "source": "/",
        "headers": [{ "key": "Link", "value": "</map.webp>; rel=preload; as=image" }]
      },
      {
        "source": "base.css",
        "headers": [
          {
            "key": "Link",
            "value": "</inconsolata-var.woff2>; rel=preload; as=font; type=font/woff2; crossorigin, </lora-var.woff2>; rel=preload; as=font; type=font/woff2; crossorigin"
          }
        ]
      },
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "OPTIONS, GET" },
          { "key": "Access-Control-Max-Age", "value": "2592000" },
          { "key": "Cache-Control", "value": "max-age=2592000" }
        ]
      },
      {
        "source": "**/*.@(css|png|webp|json)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "OPTIONS, GET" },
          { "key": "Access-Control-Max-Age", "value": "2592000" },
          { "key": "Cache-Control", "value": "max-age=2592000" }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/c0",
        "destination": "/blog/?utm_source=cv&utm_medium=link&utm_campaign=projects",
        "type": 302
      },
      {
        "source": "/{feed,feed.rss,atom,rss}",
        "destination": "/feed.atom",
        "type": 302
      },
      {
        "source": "/f",
        "destination": "/?utm_source=facebook&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/g",
        "destination": "/?utm_source=google&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/gh",
        "destination": "/?utm_source=github&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/gh0",
        "destination": "/?utm_source=github&utm_medium=repo&utm_campaign=com-seankhliao",
        "type": 302
      },
      {
        "source": "/gh1",
        "destination": "/?utm_source=github&utm_medium=profile&utm_campaign=readme",
        "type": 302
      },
      {
        "source": "/gh2",
        "destination": "/?utm_source=github&utm_medium=profile&utm_campaign=page",
        "type": 302
      },
      {
        "source": "/i",
        "destination": "/?utm_source=instagram&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/t",
        "destination": "/?utm_source=twitter&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/l",
        "destination": "/?utm_source=linkedin&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/l0",
        "destination": "/?utm_source=linkedin&utm_medium=profile&utm_campaign=about",
        "type": 302
      }
    ]
  }
}
