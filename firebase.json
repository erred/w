{
  "hosting": {
    "public": "public/",
    "cleanUrls": true,
    "trailingSlash": true,
    "headers": [
      {
        "source": "*",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000; preload"
          },
          {
            "key": "Referrer-Policy",
            "value": "strict-origin-when-cross-origin"
          },
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; style-src 'unsafe-inline'; script-src-elem  'unsafe-inline' https://www.google-analytics.com; font-src 'self' https://fonts.gstatic.com; connect-src https://*.seankhliao.com https://www.google-analytics.com; img-src *; report-uri https://statslogger.seankhliao.com/json"
          },
          {
            "key": "Feature-Policy",
            "value": "accelerometer 'none'; ambient-light-sensor 'none'; autoplay 'none'; battery 'none'; camera 'none'; display-capture 'none'; document-domain 'none'; encrypted-media 'none'; execution-while-not-rendered 'none'; execution-while-out-of-viewport 'none'; fullscreen 'none'; geolocation 'none'; gyroscope 'none'; legacy-image-formats 'none'; magnetometer 'none'; microphone 'none'; midi 'none'; navigation-override 'none'; payment 'none'; picture-in-picture 'none'; publickey-credentials-get 'none'; sync-xhr 'none'; usb 'none'; wake-lock 'none'; xr-spatial-tracking 'none'"
          }
        ]
      },
      {
        "source": "/icon-*.png",
        "headers": [{ "key": "Cache-Control", "value": "max-age=2592000" }]
      },
      {
        "source": "/blog/",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      },
      {
        "source": "/blog/**",
        "headers": [{ "key": "Cache-Control", "value": "max-age=86400" }]
      },
      {
        "source": "/",
        "headers": [{ "key": "Link", "value": "</map.webp>; rel=preload; as=image" }]
      }
    ],
    "redirects": [
      {
        "source": "/{feed,feed.rss,atom,rss}",
        "destination": "/feed.atom",
        "type": 302
      },
      {
        "source": "/l/fb/",
        "destination": "/?utm_source=facebook&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/l/gh/",
        "destination": "/?utm_source=github&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/l/gh/:repo/",
        "destination": "/?utm_source=github&utm_medium=repo&utm_campaign=:repo",
        "type": 302
      },
      {
        "source": "/l/ig/",
        "destination": "/?utm_source=instagram&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/l/tw/",
        "destination": "/?utm_source=twitter&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/l/tw/:tweet/",
        "destination": "/?utm_source=twitter&utm_medium=tweet&utm_campaign=:tweet",
        "type": 302
      },
      {
        "source": "/l/li/",
        "destination": "/?utm_source=linkedin&utm_medium=profile&utm_campaign=profile",
        "type": 302
      },
      {
        "source": "/l/li/about/",
        "destination": "/?utm_source=linkedin&utm_medium=profile&utm_campaign=about",
        "type": 302
      }
    ]
  }
}
