{
  "hosting": {
    "cleanUrls": true,
    "trailingSlash": false,
    "public": "dst/",
    "headers": [
      {
        "source": "**/*.@(ttf|woff|css)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=31536000" }]
      },
      {
        "source": "**/*.@(webp|png|ico)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=2592000" }]
      },
      {
        "source": "/blog",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      },
      {
        "source": "/blog/**",
        "headers": [{ "key": "Cache-Control", "value": "max-age=86400" }]
      },
      {
        "source": "/",
        "headers": [{ "key": "Link", "value": "</map.webp>; rel=preload; as=image" }]
      },
      {
        "source": "/authed",
        "headers": [{ "key": "Link", "value": "</authed/index.js>; rel=preload; as=script" }]
      },
      {
        "source": "/readss",
        "headers": [{ "key": "Link", "value": "</readss/index.js>; rel=preload; as=script" }]
      }
    ]
  }
}
