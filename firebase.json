{
  "hosting": {
    "cleanUrls": true,
    "trailingSlash": false,
    "public": "dst/",
    "headers": [
      {
        "source": "**/*.@(ttf|woff|css)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=31536000" }]
      },
      {
        "source": "**/*.@(webp|png)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=2592000" }]
      },
      {
        "source": "/blog",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      },
      {
        "source": "/blog/**",
        "headers": [{ "key": "Cache-Control", "value": "max-age=86400" }]
      }
    ]
  }
}
